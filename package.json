{
    "name": "@compossibru/widget-twitter-widgets",
    "homepage": "https://github.com/compossibru/widget-twitter-widgets#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/compossibru/widget-twitter-widgets.git"
    },
    "bugs": {
        "url": "https://github.com/compossibru/widget-twitter-widgets/issues"
    },
    "authors": [
        {
            "name": "Maik Schmidt",
            "email": "maik.schmidt.hl@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "twitter",
        "composite-ui",
        "compossibru",
        "compossibru-widget"
    ],
    "main": "bundled.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --open --hot",
        "prebuild": "rm -rf ./dist",
        "build": "NODE_ENV=production webpack --mode production",
        "postbuild": "rm ./dist/index.html && cp -r package.json package-lock.json README.md LICENSE ./dist"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "npx commitlint -e $HUSKY_GIT_PARAMS"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@babel/core": "7.1.6",
        "@babel/preset-env": "7.1.6",
        "@babel/preset-react": "7.0.0",
        "@commitlint/cli": "7.2.1",
        "@commitlint/config-conventional": "7.1.2",
        "@semantic-release/commit-analyzer": "6.1.0",
        "@semantic-release/git": "7.0.5",
        "@semantic-release/github": "5.2.5",
        "@semantic-release/npm": "5.1.1",
        "@semantic-release/release-notes-generator": "7.1.4",
        "babel-loader": "8.0.4",
        "html-webpack-plugin": "3.2.0",
        "husky": "1.2.0",
        "semantic-release": "15.12.4",
        "webpack": "4.26.0",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "3.1.10"
    },
    "dependencies": {
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "react-twitter-widgets": "1.7.1"
    }
}
